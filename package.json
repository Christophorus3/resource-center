    "precommit": "lint-staged"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2",
    "standard": "^11.0.1"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --single-quote",
      "standard --fix",
      "git add"
